<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.user.batch-timer.sample.every-5min</string>

    <!-- 実行するコマンド（引数は配列で指定） -->
    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>/Users/rchaser53/batch-timer/scripts/sample-task.sh</string>
    </array>

    <!-- 何秒ごとに起動するか（= 300秒ごと = 5分ごと） -->
    <key>StartInterval</key>
    <integer>300</integer>

    <!-- loadした直後にも1回実行する -->
    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Users/rchaser53/batch-timer/logs/sample.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/rchaser53/batch-timer/logs/sample.stderr.log</string>

    <key>KeepAlive</key>
    <false/>
  </dict>
</plist>
